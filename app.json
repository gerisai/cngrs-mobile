{
  "expo": {
    "name": "CNGRS",
    "slug": "CNGRS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gerisaid.cngrs",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) necesita permisos para tu cámara"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.gerisaid.cngrs"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 400,
          "resizeMode": "cover",
          "backgroundColor": "#1D1B20"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La app accede a tus fotos solo para seleccionar un avatar"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permite a $(PRODUCT_NAME) acceder a la cámara"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9520fe9-3397-4e01-b287-b89b53720489"
      }
    },
    "owner": "gerisaid"
  }
}
